{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "none"
  },
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4"
    ]
  },
  "policy": {
    "levels": {
      "1": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 2,
        "downlinkOnly": 5,
        "bufferSize": 4
      }
    }
  },
  "inbounds": [
  {
    "port": 443,
    "protocol": "vmess",
    "settings": {
      "clients": [
      {
        "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
        "level": 1,
        "alterId": 32,
        "email": "rui.tcp.v2ray@y1ph.cn"
      }
      ]
    },
    "streamSettings": {
      "network": "tcp",
      "security": "none",
      "tlsSettings": {
        "serverName": "xxx.xxx.xxx",
        "allowInsecure": false,
        "certificates": [
        {
          "certificateFile": "/etc/v2ray/v2ray.crt",
          "keyFile": "/etc/v2ray/v2ray.key"
        }
        ]
      },
      "sockopt": {
        "mark": 0,
        "tcpFastOpen": true
      }
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  },
  {
    "port": 8912,
    "protocol": "vmess",
    "settings": {
      "clients": [
      {
        "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
        "level": 1,
        "alterId": 32,
        "email": "tcp.v2ray@y1ph.cn"
      }
      ]
    },
    "detour":{
      "to": "dynamicPort"
    },
    "streamSettings": {
      "network": "tcp",
      "security": "none",
      "tlsSettings": {
        "serverName": "xxx.xxx.xxx",
        "allowInsecure": false,
        "certificates": [
        {
          "certificateFile": "/etc/v2ray/v2ray.crt",
          "keyFile": "/etc/v2ray/v2ray.key"
        }
        ]
      },
      "sockopt": {
        "mark": 0,
        "tcpFastOpen": true
      }
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  },
  {
    "port": "30000-40000",
    "protocol": "vmess",
    "tag": "dynamicPort",
    "settings": {
      "default": {
        "alterId": 32
      }
    },
    "allocate": {
      "strategy": "random",
      "concurrency": 2,
      "refresh": 3
    },
    "streamSettings": {
      "network": "tcp",
      "sockopt": {
        "mark": 0,
        "tcpFastOpen": true
      }
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  },
  {
    "port": 8914,
    "protocol": "vmess",
    "settings": {
      "clients": [
      {
        "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
        "level": 1,
        "alterId": 64,
        "email": "phone.tcp.v2ray@y1ph.cn"
      }
      ]
    },
    "streamSettings": {
      "network": "tcp",
      "security": "none",
      "tlsSettings": {
        "serverName": "xxx.xxx.xxx",
        "allowInsecure": false,
        "certificates": [
        {
          "certificateFile": "/etc/v2ray/v2ray.crt",
          "keyFile": "/etc/v2ray/v2ray.key"
        }
        ]
      },
      "sockopt": {
        "mark": 0,
        "tcpFastOpen": true
      }
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  },
  {
    "protocol": "vmess",
    "port": 8913,
    "settings": {
      "clients": [
      {
        "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
        "level": 1,
        "alterId": 32,
        "email": "kcp.v2ray@y1ph.cn"
      }
      ]
    },
    "streamSettings": {
      "network": "kcp"
    },
    "sniffing": {
      "enabled": true,
      "destOverride": ["http", "tls"]
    }
  }
  ],
  "outbounds": [
  {
    "protocol": "freedom",
    "settings": {}
  },
  {
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }
  ],
  "routing": {
    "strategy": "rules",
    "domainStrategy": "IPOnDemand",
    "settings": {
      "rules": [
        {
          "type": "field",
          "ip": [
            "0.0.0.0/8",
            "10.0.0.0/8",
            "100.64.0.0/10",
            "127.0.0.0/8",
            "169.254.0.0/16",
            "172.16.0.0/12",
            "192.0.0.0/24",
            "192.0.2.0/24",
            "192.168.0.0/16",
            "198.18.0.0/15",
            "198.51.100.0/24",
            "203.0.113.0/24",
            "::1/128",
            "fc00::/7",
            "fe80::/10"
          ],
          "outboundTag": "blocked"
        }
      ]
    }
  },
  "transport": {
    "tcpSettings": {
      "header": {
        "type": "none"
      }
    },
    "kcpSettings": {
      "mtu": 1350,
      "tti": 20,
      "uplinkCapacity": 2,
      "downlinkCapacity": 20,
      "congestion": false,
      "readBufferSize": 1,
      "writeBufferSize": 1,
      "header": {
        "type": "dtls"
      }
    }
  }
}
